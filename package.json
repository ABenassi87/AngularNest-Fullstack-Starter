{
  "name": "Angular-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate written in Pug, Stylus, and TypeScript using Angular, ReactiveX, and NgRx.",
  "scripts": {
    "start": "tsc webpack.dev.ts && webpack-dev-server --config webpack.dev.js --inline --progress --port 3001",
    "build": "tsc webpack.prod.ts && rimraf public && webpack --config webpack.prod.js --progress --profile --bail",
    "lint": "gulp lint",
    "unit": "tsc karma.conf.ts test-shim.ts && karma start",
    "e2e": "tsc protractor.conf.ts && gulp e2e",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "npm cache clean && git clean -dfx -e '.*' && npm install",
    "postinstall": "npm run gulp -- webdriver",
    "gulp": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular-Starter-Kit.git"
  },
  "keywords": [
    "Angular"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular-Starter-Kit#readme",
