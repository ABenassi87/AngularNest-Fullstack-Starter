{
  "name": "Universal-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Material, Firebase, Express, Mongoose, and Google Cloud Platform.",
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@angular/animations": "4.2.4",
    "@angular/common": "4.2.4",
    "@angular/compiler": "4.2.4",
    "@angular/core": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/platform-server": "4.2.4",
    "@angular/router": "4.2.4",
    "@nguniversal/express-engine": "1.0.0-beta.2",
    "express": "4.15.3",
    "rxjs": "5.4.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.2.4",
    "@ngtools/webpack": "1.4.2",
    "@types/express": "4.0.36",
    "@types/node": "8.0.4",
    "html-webpack-plugin": "2.29.0",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "script-ext-html-webpack-plugin": "1.8.3",
    "typescript": "2.4.1",
    "webpack": "3.0.0",
    "webpack-merge": "4.1.0"
  }
}
