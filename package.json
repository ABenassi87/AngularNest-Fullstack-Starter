{
  "name": "Angular2TS-Starter-Kit",
  "version": "1.0.0",
  "description": "A starter kit for Angular 2 applications with TypeScript",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "webpack --config tools/config/webpack.prod.js --progress --profile --bail",
    "lint": "gulp lint",
    "unit": "karma start",
    "e2e": "gulp e2e",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "npm cache clean && git clean -dfx -e '.*' && npm install",
    "postinstall": "npm run typings -- install && npm run gulp -- webdriver",
    "gulp": "gulp",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular2TS-Starter-Kit.git"
  },
  "keywords": [
    "Angular 2",
    "TypeScript"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular2TS-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular2TS-Starter-Kit#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular2-material/button": "^2.0.0-alpha.6",
    "@angular2-material/core": "^2.0.0-alpha.6",
    "angularfire2": "^2.0.0-beta.2",
    "core-js": "2.4.1",
    "firebase": "^3.2.1",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.13"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "browser-sync": "^2.14.0",
    "codelyzer": "^0.0.28",
    "coffee-script": "^1.10.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.24.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-protractor": "^2.4.0",
    "gulp-pug-linter": "^0.3.0",
    "gulp-stylint": "^3.0.0",
    "gulp-tslint": "^6.0.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.12",
    "poststylus": "^0.2.3",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rucksack-css": "^0.8.6",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "systemjs-builder": "^0.15.26",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.3",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.2",
