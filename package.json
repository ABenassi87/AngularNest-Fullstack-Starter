{
  "name": "Angular-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Material, Firebase, Express, Mongoose, and Google Cloud Platform.",
  "scripts": {
    "start": "ng serve",
    "test": "jest --coverage",
    "build:web": "ng build -prod",
    "build:backend": "webpack -p",
