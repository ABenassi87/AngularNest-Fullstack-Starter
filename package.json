{
  "name": "Angular-Nest-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Nest, TypeScript, and ReactiveX.",
  "scripts": {
    "start:app": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "start:api": "nodemon src/main.server.ts --exec ts-node",
    "build:app": "cross-env BUILD_AOT=1 webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:api": "tsc -p tsconfig.api.json",
    "test:app": "cross-env TEST_ENV=app jest \"src/app/.*\\.ts$\" --coverage --forceExit",
    "test:api": "cross-env TEST_ENV=api jest \"src/api/.*\\.ts$\" --coverage --forceExit",
    "deploy": "firebase deploy",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular-Nest-Starter.git"
  },
  "keywords": [
    "Angular",
    "Nest",
    "TypeScript",
    "ReactiveX"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular-Nest-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular-Nest-Starter#readme",
