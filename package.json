{
  "name": "Angular-FullStarter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Material, and Express.",
  "scripts": {
    "start": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "build": "cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "test": "tslint src && jest --coverage --forceExit",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "deploy": "firebase deploy",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular-Fullstack-Starter.git"
  },
  "keywords": [
    "Angular"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular-Fullstack-Starter#readme",
