{
  "name": "Angular2TS-Starter-Kit",
  "version": "1.0.0",
  "description": "A starter kit for Angular 2 applications with TypeScript",
  "scripts": {
    "start": "rimraf public && gulp",
    "test": "gulp build && gulp e2e",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "npm cache clean && git clean -dfx -e '.*' && npm install",
    "postinstall": "npm run typings -- install && gulp postinstall",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular2TS-Starter-Kit.git"
  },
  "keywords": [
    "Angular 2",
    "TypeScript"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular2TS-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular2TS-Starter-Kit#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "es6-module-loader": "0.17.8",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "browser-sync": "^2.12.5",
    "codelyzer": "0.0.19",
    "coffee-script": "^1.10.0",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-protractor": "^2.3.0",
    "gulp-pug": "^2.0.0",
    "gulp-pug-linter": "^0.2.1",
    "gulp-remember": "^0.3.1",
    "gulp-stylint": "^3.0.0",
    "gulp-stylus": "^2.3.1",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.0",
    "jasmine-spec-reporter": "^2.4.0",
    "poststylus": "^0.2.3",
    "rimraf": "^2.5.2",
    "rucksack-css": "^0.8.6",
    "run-sequence": "^1.1.5",
    "ts-node": "^0.7.2",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
