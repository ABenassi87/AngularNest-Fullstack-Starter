  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Angular-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Angular-Starter-Kit#readme",
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.ts"
  },
